@model List<IntroMVC.Models.Person>
@{
    ViewBag.Title = "List";
    Layout = "~/Views/Shared/_loggedIn.cshtml";
}

<h2>List</h2>
<table class="table table-bordered">
    <tr>
        <th>Name</th>
        <th>Id</th>
        <th>Dob</th>
    </tr>
    @foreach (var p in Model)
    {
    <tr>
        <td>@p.Name</td>
        <td>@p.Id</td>
        <td>@p.Dob</td>
    </tr>
    }

</table>

